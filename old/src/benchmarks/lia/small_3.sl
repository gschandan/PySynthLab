(set-logic LIA)
(synth-fun max2 ((a Int) (b Int)) Int)
(declare-var x Int)
(declare-var y Int)
(constraint (>= (max2 x y) x))
(constraint (>= (max2 x y) y))
(constraint (or (= x (max2 x y)) (= y (max2 x y))))
(constraint (= (max2 x x) x))
(declare-var a Int)
(declare-var b Int)
(constraint (=> (and (>= a b) (>= b a)) (= (max2 a b) a)))
(constraint (=> (and (>= b a) (>= a b)) (= (max2 b a) b)))
(check-synth)